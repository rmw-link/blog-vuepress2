#!/usr/bin/env bash

branch=`git branch 2> /dev/null | sed -e '/^[^*]/d' |awk -F' ' '{print $2}'`

git add --update :/ && git commit -m "fix: ðŸ”¶" 

git pull origin $branch

branch=`git branch|awk '{print $2}'`

git pull origin $branch

git merge $branch

git push --tag --set-upstream origin $branch 
